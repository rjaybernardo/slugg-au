        <script>
        jQuery(document).on('change', '.product_option_main .variant_common_option', function(){
            $this = jQuery(this);
            $this.parents('.product_option_main').find('.swatch-view-item').removeClass('active');
            $this.parent('.swatch-view-item').addClass('active');
            var dataVariantPrice = $this.parent('.swatch-view-item').find('.swatch_img').attr('data-variant-price');
            var dataVariantUrl = $this.parent('.swatch-view-item').find('.swatch_img').attr('data-variant-url');
            var optionDataImg = $this.parent('.swatch-view-item').find('.swatch_img').attr('option-data-img');
            
            $this.parents('.product_tile_main').find('.hover_img img').attr('src', optionDataImg);
            $this.parents('.product_tile_main').find('.hover_img img').attr('srcset', optionDataImg);
            $this.parents('.product_tile_main').find('.product_price').text(dataVariantPrice);
            $this.parents('.product_tile_main').find('.shop_btn').attr('href', dataVariantUrl);
